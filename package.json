{
  "name": "adapcon-utils-js",
  "version": "0.0.9",
  "description": "utils for javascript",
  "main": "dist/adapcon-utils-js.ssr.js",
  "browser": "dist/adapcon-utils-js.esm.js",
  "module": "dist/adapcon-utils-js.esm.js",
  "unpkg": "dist/adapcon-utils-js.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tdd": "npm run test --watch",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adapcon-team/adapcon-utils-js.git"
  },
  "keywords": [
    "utils",
    "js"
  ],
  "author": "adapcon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adapcon-team/adapcon-utils-js/issues"
  },
  "homepage": "https://github.com/adapcon-team/adapcon-utils-js#readme",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-replace": "^3.0.0",
    "aws-sdk": "^2.999.0",
    "core-js": "^3.18.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.4",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
